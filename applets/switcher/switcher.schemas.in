<gconfschemafile>
    <schemalist>
    
      <schema>
        <key>/schemas/apps/awn-switcher/prefs/n_rows</key>
        <owner>awn-switcher</owner>
        <type>int</type>
        <default>1</default>
        <locale name="C">
          <short>How many rows the switcher should have</short>
          <long></long>
        </locale>
      </schema>
       <schema>
        <key>/schemas/apps/awn-switcher/prefs/width</key>
        <owner>awn-switcher</owner>
        <type>int</type>
        <default>100</default>
        <locale name="C">
          <short>The width of the switcher</short>
          <long></long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/awn-switcher/prefs/height</key>
        <owner>awn-switcher</owner>
        <type>int</type>
        <default>24</default>
        <locale name="C">
          <short>The height of the switcher</short>
          <long></long>
        </locale>
      </schema>
      
       
    </schemalist>

</gconfschemafile>  
