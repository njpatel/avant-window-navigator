<?xml version="1.0"?>
<gconfschemafile>
<schemalist>
	<schema>
		<key>/schemas/apps/affinity/bar/rounded_corners</key>
		<applyto>/apps/affinity/bar/rounded_corners</applyto>
		<owner>affinity</owner>
		<type>bool</type>
		<default>true</default>
		<locale name="C">
			<short>Bar has rounded corners.</short>
			<long>If enabled, the bar will have rounded corners.</long>
		 </locale>
	</schema>
	<schema>
		<key>/schemas/apps/affinity/bar/render_pattern</key>
		<applyto>/apps/affinity/bar/render_pattern</applyto>
		<owner>affinity</owner>
		<type>bool</type>
		<default>false</default>
		<locale name="C">
			<short>Renders the chosen pattern.</short>
			<long>If enabled, the pattern from pattern_uri will be rendered.</long>
		 </locale>
	</schema>
	<schema>
		<key>/schemas/apps/affinity/bar/pattern_uri</key>
		<applyto>/apps/affinity/bar/pattern_uri</applyto>
		<owner>affinity</owner>
		<type>string</type>
		<default>/usr/share/nautilus/patterns/terracotta.png</default>
		<locale name="C">
			<short>The PNG pattern to be rendered</short>
			<long>URI for the PNG pattern to be rendered.</long>
		 </locale>
	</schema>
	<schema>
		<key>/schemas/apps/affinity/bar/pattern_alpha</key>
		<applyto>/apps/affinity/bar/pattern_alpha</applyto>
		<owner>affinity</owner>
		<type>float</type>
		<default>1.0</default>
		<locale name="C">
			<short>Transparency of pattern.</short>
			<long>Transparency of pattern, between 0.0 (fully transparent) - 1.0 (no transparency).</long>
		 </locale>
	</schema>
</schemalist>
</gconfschemafile>
